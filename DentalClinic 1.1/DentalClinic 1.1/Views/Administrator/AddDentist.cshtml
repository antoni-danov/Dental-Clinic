    <label for="email"><b>Town</b></label>
    <input type="text" placeholder="Town" name="town" id="town" required>
    <label for="email"><b>PhoneNumber</b></label>
    <input type="text" placeholder="Phone number" name="phone number" id="phone number" required>
    <label for="email"><b>Autobiographie</b></label>
    <textarea id="description" name="description"
          rows="5" cols="33">

