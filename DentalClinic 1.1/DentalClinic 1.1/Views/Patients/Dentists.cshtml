@model IEnumerable<DentalClinic_1._1.ViewModels.Dentist.AllDentistsViewModel>

@{
    ViewData["Title"] = "All Dentists";
}

<h1>Dentists in our Clinic</h1>


    @foreach (var item in Model)
    {
        <div class="card mb-3">

            <button class="card-title btn-primary text-white">@item.FirstName @item.LastName</button>
        </div>
    }
